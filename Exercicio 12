public class Atv12 {

    

    public static void imprimirSoma(){
        /* declaro a variavel aqui dentro, pois se declarar fora desse
        void, nao funciona! */

        int a = 10;
        int b = 20;

        System.out.print(a + b);
    
    }
    public static void main(String[] args) {
         
        imprimirSoma();
       
    }
    
}
